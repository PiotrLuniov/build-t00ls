      <projectUrl>https://github.com/MNT-Lab/build-t00ls/</projectUrl>
      <displayName>home-task</displayName>
    </com.coravy.hudson.plugins.github.GithubProjectProperty>
    <hudson.plugins.copyartifact.CopyArtifactPermissionProperty plugin="copyartifact@1.42.1">
