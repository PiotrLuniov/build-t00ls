    <hudson.tasks.Shell>
      <command>#!/bin/sh
java -cp home-task/target/my-project-1.0-SNAPSHOT.jar com.test.Project &gt; output.log
tar -cf hello-$BUILD_NUMBER.tar.gz home-task/target/my-project-1.0-SNAPSHOT.jar
mkdir $JENKINS_HOME/artifacts1
cp hello-$BUILD_NUMBER.tar.gz $JENKINS_HOME/artifacts1
